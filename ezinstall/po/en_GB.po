#: ezinstall.sh:29
#, sh-format
msgid "Will create subvolumes: ${btsubvols}"
msgstr "Will create subvolumes: ${btsubvols}"

#: ezinstall.sh:44
msgid "Select filesystem to use for Lirix"
msgstr "Select filesystem to use for Lirix"

#: ezinstall.sh:44
msgid "Stable filesystem that uses B-Trees. Very good."
msgstr "Stable filesystem that uses B-Trees. Very good."

#: ezinstall.sh:44
msgid "Default filesystem for SGI's IRIX. Journaling cannot be disabled."
msgstr "Default filesystem for SGI's IRIX. Journaling cannot be disabled."

#: ezinstall.sh:44
msgid ""
"Default filesystem for many Linux distributions. Use if uncomfortable with "
"other options."
msgstr ""
"Default filesystem for many Linux distributions. Use if uncomfortable with "
"other options."

#: ezinstall.sh:44
msgid "Manually format partition using a shell."
msgstr "Manually format partition using a shell."

#: ezinstall.sh:52
msgid "Created XFS filesystem on $1"
msgstr "Created XFS filesystem on $1"

#: ezinstall.sh:58
msgid "Created EXT4 filesystem on $1"
msgstr "Created EXT4 filesystem on $1"

#: ezinstall.sh:63 ezinstall.sh:259
msgid ">> Entering interactive Bourne Again Shell."
msgstr ">> Entering interactive Bourne Again Shell."

#: ezinstall.sh:64
msgid ">> Format partition $1 in whichever way you desire."
msgstr ">> Format partition $1 in whichever way you desire."

#: ezinstall.sh:65
msgid ">> The mountpoint for your Lirix installation is /mnt/lirix"
msgstr ">> The mountpoint for your Lirix installation is /mnt/lirix"

#: ezinstall.sh:66
msgid ">> Mount $1 to there when you have completed your manual formatting."
msgstr ">> Mount $1 to there when you have completed your manual formatting."

#: ezinstall.sh:67
msgid ">> To return to EZInstall, type exit."
msgstr ">> To return to EZInstall, type exit."

#: ezinstall.sh:78
msgid "EZAutopartitioning selected.\\nWill partition device $1"
msgstr "EZAutopartitioning selected.\\nWill partition device $1"

#: ezinstall.sh:80
msgid "UEFI detected. Will use GPT for partitioning."
msgstr "UEFI detected. Will use GPT for partitioning."

#: ezinstall.sh:81 ezinstall.sh:124
msgid "Specify swap size in MiB?"
msgstr "Specify swap size in MiB?"

#: ezinstall.sh:84 ezinstall.sh:127
msgid "Enter swap size"
msgstr "Enter swap size"

#: ezinstall.sh:86 ezinstall.sh:129
msgid "Swap size must be an integer!"
msgstr "Swap size must be an integer!"

#: ezinstall.sh:92 ezinstall.sh:135
msgid "Selecting yes now WILL cause data loss.\\nContinue?"
msgstr "Selecting yes now WILL cause data loss.\\nContinue?"

#: ezinstall.sh:113 ezinstall.sh:151
msgid "EZAutopartitioning complete."
msgstr "EZAutopartitioning complete."

#: ezinstall.sh:123
msgid "LegacyBIOS detected. Will use MBR for partitioning."
msgstr "LegacyBIOS detected. Will use MBR for partitioning."

#: ezinstall.sh:165
msgid "Enter username for new user"
msgstr "Enter username for new user"

#: ezinstall.sh:167
msgid ""
"Username must only contain lowercase letters or the dash (-) symbol and must "
"not be empty."
msgstr ""
"Username must only contain lowercase letters or the dash (-) symbol and must "
"not be empty."

#: ezinstall.sh:176
#, sh-format
msgid "Enter password for user ${newlirixuser}\\n(default is apioforms)"
msgstr "Enter password for user ${newlirixuser}\\n(default is apioforms)"

#: ezinstall.sh:182 ezinstall.sh:308
msgid "Me password again."
msgstr "Me password again."

#: ezinstall.sh:184 ezinstall.sh:310
msgid "Passwords do NOT match!!"
msgstr "Passwords do NOT match!!"

#: ezinstall.sh:188
#, sh-format
msgid "Enter description/full name for user ${newlirixuser}"
msgstr "Enter description/full name for user ${newlirixuser}"

#: ezinstall.sh:191
msgid "Do you wish to enable administrative (sudo) privileges for this user?"
msgstr "Do you wish to enable administrative (sudo) privileges for this user?"

#: ezinstall.sh:195
#, sh-format
msgid "Enter home directory for user ${newlirixuser}"
msgstr "Enter home directory for user ${newlirixuser}"

#: ezinstall.sh:197
msgid "Do you want this user to be able to login?"
msgstr "Do you want this user to be able to login?"

#: ezinstall.sh:213
msgid "Welcome to EZInstall, the installer for Lirix!"
msgstr "Welcome to EZInstall, the installer for Lirix!"

#: ezinstall.sh:214
msgid "Would you like to install Lirix at this moment?"
msgstr "Would you like to install Lirix at this moment?"

#: ezinstall.sh:219
msgid "Select your keymap"
msgstr "Select your keymap"

#: ezinstall.sh:223
msgid "Select installation disk"
msgstr "Select installation disk"

#: ezinstall.sh:226
#, sh-format
msgid "Do you wish to partition $device?"
msgstr "Do you wish to partition $device?"

#: ezinstall.sh:227
#, sh-format
msgid "Do you wish to autopartition $device?"
msgstr "Do you wish to autopartition $device?"

#: ezinstall.sh:239
msgid "Select boot partition"
msgstr "Select boot partition"

#: ezinstall.sh:241
msgid "Select swap partition"
msgstr "Select swap partition"

#: ezinstall.sh:242
msgid "Select Lirix partition"
msgstr "Select Lirix partition"

#: ezinstall.sh:258
msgid ""
"Would you like to enter an interactive shell to manually configure more "
"advanced options before proceeding?"
msgstr ""
"Would you like to enter an interactive shell to manually configure more "
"advanced options before proceeding?"

#: ezinstall.sh:260
msgid ">> The root mountpoint of the Lirix installation is /mnt/lirix"
msgstr ">> The root mountpoint of the Lirix installation is /mnt/lirix"

#: ezinstall.sh:261
msgid ">> Once complete, return to EZInstall by typing exit."
msgstr ">> Once complete, return to EZInstall by typing exit."

#: ezinstall.sh:266
msgid "Starting installation of Lirix. Setup will continue shortly hereafter."
msgstr "Starting installation of Lirix. Setup will continue shortly hereafter."

#: ezinstall.sh:268
msgid "Installation complete. Setup will now continue."
msgstr "Installation complete. Setup will now continue."

#: ezinstall.sh:276
msgid "Enter hostname for system\\n(default is apioform-hive)"
msgstr "Enter hostname for system\\n(default is apioform-hive)"

#: ezinstall.sh:278
msgid "Hostname must only contain lowercase letters or the dash (-) symbol."
msgstr "Hostname must only contain lowercase letters or the dash (-) symbol."

#: ezinstall.sh:288
msgid "Enter username for main user\\n(default is aamoo)"
msgstr "Enter username for main user\\n(default is aamoo)"

#: ezinstall.sh:290
msgid "Username must only contain lowercase letters or the dash (-) symbol."
msgstr "Username must only contain lowercase letters or the dash (-) symbol."

#: ezinstall.sh:302
#, sh-format
msgid "Enter password for user ${lirixuser}\\n(default is apioforms)"
msgstr "Enter password for user ${lirixuser}\\n(default is apioforms)"

#: ezinstall.sh:314
#, sh-format
msgid "Enter description/full name for user ${lirixuser}"
msgstr "Enter description/full name for user ${lirixuser}"

#: ezinstall.sh:326
msgid "Do you wish to add an additional user?"
msgstr "Do you wish to add an additional user?"

#: ezinstall.sh:334
msgid "Select timezone"
msgstr "Select timezone"

#: ezinstall.sh:345
msgid ""
"Select locale. To check a box, press the spacebar key. When you have "
"completed your selection of locales, press the enter key."
msgstr ""
"Select locale. To check a box, press the spacebar key. When you have "
"completed your selection of locales, press the enter key."

#: ezinstall.sh:354
msgid "Select preferred locale"
msgstr "Select preferred locale"

